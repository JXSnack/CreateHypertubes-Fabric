{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [${mod_authors}],
    "contact": {
        "sources": "https://github.com/Fabricators-of-Create/create-fabric-addon-template"
    },
    "license": "${mod_license}",
    "icon": "assets/${mod_id}/icon.png",
    "environment": "*",
    "entrypoints": {
        "main": ["com.pedrorok.hypertube.HypertubeMod"],
        "client": ["com.pedrorok.hypertube.HypertubeModClient"]
    },
    "mixins": [
        "create_hypertube.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${loader_version}",
        "fabric-api": ">=${fabric_api_version}",
        "create": "${create_version}",
        "minecraft": "${minecraft_version}"
    }
}